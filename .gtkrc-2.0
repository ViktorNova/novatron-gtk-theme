# DO NOT EDIT! This file will be overwritten by LXAppearance.
# Any customization should be done in ~/.gtkrc-2.0.mine instead.

gtk-theme-name="Orta"
gtk-icon-theme-name="Oxygen Mono Light"
gtk-font-name="Enter Sansman Bold 11"
gtk-cursor-theme-name="oxy-oxygen"
gtk-cursor-theme-size=0
gtk-toolbar-style=GTK_TOOLBAR_ICONS
gtk-toolbar-icon-size=GTK_ICON_SIZE_SMALL_TOOLBAR
gtk-button-images=0
gtk-menu-images=0
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="hintfull"
gtk-xft-rgba="rgb"
gtk-color-scheme="tooltip_fg_color:#00FFFF\nlink_color:#00FFFF\ninactive_frame_color:#0044cc\nbase_color:#000000\nframe_color:#f0f0f1\nselected_fg_color:#000000\ntext_color:#0055FF\nbg_color_dark:#000000\ntext_color_dark:#0055FF\nbg_color:#000000\ntooltip_bg_color:#000\nsidebar_color:#0055FF\nselected_bg_color:#0055FF\nfg_color:#0055FF\n"
include "/home/guest/.gtkrc-2.0.mine"
